{
  "name": "@qwezey/typescript",
  "type": "module",
  "engines": {
    "node": "22"
  },
  "version": "0.0.9",
  "description": "QweZey's opinionated typescript/eslint/prettier config for any typescript project",
  "exports": {
    ".": "./tsconfig.json",
    "./eslint": "./dist/eslint.js",
    "./prettier": "./dist/prettier.js"
  },
  "bin": {
    "qwezey-typescript": "./dist/bin/index.js"
  },
  "files": [
    "dist",
    "src",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json && npx -yp @vercel/ncc build src/bin.ts -o dist/bin -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qwezey/typescript.git"
  },
  "keywords": [
    "typescript",
    "eslint",
    "prettier",
    "config",
    "opinionated",
    "strict"
  ],
  "author": "QweZey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/qwezey/typescript/issues"
  },
  "homepage": "https://github.com/qwezey/typescript#readme",
  "dependencies": {
    "@eslint/js": "^9.37.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1"
  },
  "devDependencies": {
    "@actions/exec": "^1.1.1",
    "@types/node": "^22",
    "commander": "^14.0.1"
  }
}
